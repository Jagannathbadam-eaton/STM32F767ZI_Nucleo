/* Standard includes. */
#include <stdint.h>
#include <stdio.h>

/* FreeRTOS includes. */
#include "../../../../../../../../../External/FreeRTOS/Source/include/FreeRTOS.h"
#include "../../../../../../../../../External/FreeRTOS/Source/include/FreeRTOS_IP.h"
#include "../../../../../../../../../External/FreeRTOS-Plus/Source/FreeRTOS-Plus-TCP/include/FreeRTOS_ARP.h"


void harness()
{
    const MACAddress_t xMACAddress;

    /* The pointer passed to ulARPRemoveCacheEntryByMac cannot be NULL
     * (see the API definition). */
    ulARPRemoveCacheEntryByMac( &xMACAddress );
}
