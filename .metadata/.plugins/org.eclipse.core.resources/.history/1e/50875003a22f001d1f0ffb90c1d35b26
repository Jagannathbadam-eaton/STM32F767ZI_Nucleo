#include "../../../../../../../External/FreeRTOS-Plus/Test/CMock/examples/temp_sensor/src/Types.h"
#include "../../../../../../../External/FreeRTOS-Plus/Test/CMock/examples/temp_sensor/src/UsartTransmitBufferStatus.h"
#include "../../../../../../../External/FreeRTOS-Plus/Test/CMock/vendor/unity/src/unity.h"

AT91S_USART Usart0Peripheral;

void setUp(void)
{
}

void tearDown(void)
{
}

void testReadyToTransmitShouldReturnStatusPerTransmitBufferReadyStatus(void)
{
  AT91C_BASE_US0->US_CSR = 0;
  TEST_ASSERT(!Usart_ReadyToTransmit());
  
  AT91C_BASE_US0->US_CSR = AT91C_US_TXRDY;
  TEST_ASSERT(Usart_ReadyToTransmit());
}
